{
    "openapi": "3.0.2",
    "info": {
        "title": "Salesforce brukernotifikasjon API",
        "version": "2.0"
    },
    "servers": [
        { "url": "https://sf-brukernotifikasjon-v2.dev.intern.nav.no" }
    ],
    "paths": {
        "/done": {
            "post": {
                "tags": ["Done"],
                "description": "Endepunkt for å sende Done til brukernotifikasjon",
                "parameters": [
                    {
                        "name": "eventId",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "description": "Unik ID til notifikasjonen"
                    }
                ],
                "requestBody": {
                    "required": true,
                    "description": "Kafka parametrar",
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "array",
                                "items": {
                                    "$ref": "#/components/schemas/DoneRequest"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                }
            }
        },
        "/innboks": {
            "post": {
                "tags": ["Innboks"],
                "description": "Endepunkt for å sende Innboks til brukernotifikasjon",
                "parameters": [
                    {
                        "name": "eventId",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "description": "Unik ID til notifikasjonen"
                    }
                ],
                "requestBody": {
                    "required": true,
                    "description": "Kafka parametrar",
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "array",
                                "items": {
                                    "$ref": "#/components/schemas/InnboksRequest"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "DoneRequest": {
                "type": "object",
                "properties": {
                    "tidspunkt": {
                        "type": "string",
                        "description": "Datetime string",
                        "format": "date-time",
                        "example": "2021-06-27T12:00:00.000Z"
                    },
                    "fodselsnummer": {
                        "type" : "string",
                        "description" : "fodselsnummer"
                    },
                    "grupperingsId": {
                        "type" : "string",
                        "description" : "grupperingsId"
                    }
                }
            },
            "InnboksRequest": {
                "type": "object",
                "properties": {
                    "eksternVarsling": {
                        "type": "boolean",
                        "description": "eksternVarsling",
                        "example": false
                    },
                    "link": {
                        "type" : "string",
                        "description" : "link",
                        "example": ""
                    },
                    "sikkerhetsnivaa": {
                        "type" : "integer",
                        "description" : "sikkerhetsnivaa",
                        "example": 4
                    },
                    "tekst": {
                        "type" : "string",
                        "description" : "tekst",
                        "example": "tekst"
                    },
                    "prefererteKanaler" : {
                        "type" : "string",
                        "description" : "csv med prefererteKanaler",
                        "example" : "SMS,EPOST"
                    },
                    "tidspunkt": {
                        "type": "string",
                        "description": "Datetime string",
                        "format": "date-time",
                        "example": "2021-06-27T12:00:00.000Z"
                    },
                    "fodselsnummer": {
                        "type" : "string",
                        "description" : "fodselsnummer",
                        "example" : "27098312345"
                    },
                    "grupperingsId": {
                        "type" : "string",
                        "description" : "grupperingsId",
                        "example" : "grupperingsId"
                    },
                    "epostVarslingstekst": {
                        "type" : "string",
                        "description" : "epostVarslingstekst",
                        "example" : "epostVarslingstekst"
                    },
                    "epostVarslingstittel": {
                        "type" : "string",
                        "description" : "epostVarslingstittel",
                        "example" : "epostVarslingstittel"
                    },
                    "smsVarslingstekst": {
                        "type" : "string",
                        "description" : "smsVarslingstekst",
                        "example" : "smsVarslingstekst"
                    }
                }
            }
        }
    }
}
