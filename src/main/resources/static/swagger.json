{
    "openapi": "3.0.2",
    "info": {
        "title": "Salesforce brukernotifikasjon API",
        "version": "1.0"
    },
    "servers": [
        { "url": "https://sf-brukernotifikasjon.dev.intern.nav.no" }
    ],
    "paths": {
        "/send": {
            "post": {
                "tags": ["Send"],
                "description": "Endepunkt for post til brukernotifikasjon",
                "requestBody": {
                    "required": true,
                    "description": "Kafka parametrar",
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/SendRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
        "/done": {
            "post": {
                "tags": ["Done"],
                "description": "Endepunkt for Ã¥ sende Done til brukernotifikasjon",
                "parameters": [
                    {
                        "name": "eventId",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "description": "Unik ID til notifikasjonen"
                    }
                ],
                "requestBody": {
                    "required": true,
                    "description": "Kafka parametrar",
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/DoneRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "SendRequest": {
                "type": "object",
                "properties": {
                    "hej": {
                        "type" : "string",
                        "description" : "desc"
                    }
                }
            },
            "DoneRequest": {
                "type": "object",
                "properties": {
                    "tidspunkt": {
                        "type": "string",
                        "description": "Datetime string",
                        "format": "date-time",
                        "example": "2021-06-27T12:00:00.000Z"
                    },
                    "fodselsnummer": {
                        "type" : "string",
                        "description" : "fodselsnummer"
                    },
                    "grupperingsId": {
                        "type" : "string",
                        "description" : "grupperingsId"
                    }
                }
            }
        }
    }
}
